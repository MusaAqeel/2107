{
    "version": 2,
    "builds": [
      {
        "src": "api/index.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/api/spotify/create-playlist/(.*)",
        "dest": "api/index.py"
      },
      {
        "src": "/api/ai-chat/generate/(.*)",
        "dest": "api/index.py"
      }
    ]
  }
  